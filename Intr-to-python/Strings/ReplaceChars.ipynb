   "source": [
    "def replace(str, char1, char2):\n",
    "    new_str = ''\n",
    "    for i in str:\n",
    "        if i == char1:\n",
    "            new_str += char2\n",
    "        else:\n",
    "            new_str += i\n",
    "    return new_str\n",
    "\n",
    "str = 'mynameismayank'\n",
    "str = replace(str,'a', 'l')\n",
    "print(str)"
   ]
